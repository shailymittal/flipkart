<% @users.each do |element| %>
  <li>
  	<%= "Id: "%>
  	<%= element['id'] %>
  	<%= "Name: "%>
  	<%= element['name'] %>
  	<%= "Contact No: "%>
  	<%= element['contact_no'] %>
  	<%= "Email: "%>
  	<%= element['email'] %>
  </li>
  
<% end %>

<%= form_tag(:action => 'write') do %>
  <p>
  	√èd:
