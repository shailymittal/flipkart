  </p>
  <p>
  	Name:
    <%= text_field_tag('name',params[:name])%>
  </p>
  <p>
  	Contact No:
    <%= text_field_tag('contact_no',params[:contact_no])%>
  </p>
  <p>
  	Email Id:
    <%= text_field_tag('email',params[:email])%>
  </p>
  <p>
    <%= submit_tag("Submit Entry")%>
  </p>
<% end %>